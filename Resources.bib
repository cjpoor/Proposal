  shortjournal = {Culture Psychology},
  author = {Billett, Stephen},
  urldate = {2016-05-12},
  date = {2003-01-06},
  pages = {133--169},
  keywords = {appropriation,cultural practice,goal-directed activity,hairdressing},
  file = {Billett_2003_Sociogeneses, Activity and Ontogeny.pdf:/Users/christopherpoor/Library/Application Support/Firefox/Profiles/nhlkm2qb.default-1455547641581/zotero/storage/5RVBA49G/Billett_2003_Sociogeneses, Activity and Ontogeny.pdf:application/pdf;Snapshot:/Users/christopherpoor/Library/Application Support/Firefox/Profiles/nhlkm2qb.default-1455547641581/zotero/storage/ESK77ZAC/133.html:text/html}
