  author = {Hodara, Michelle},
  urldate = {2016-05-23},
  date = {2011-02},
  keywords = {Cognitive Processes,College Mathematics,Community Colleges,Computer Assisted Instruction,Computer Uses in Education,Cooperative Learning,Developmental Studies Programs,Educational Change,Evidence,Instructional Effectiveness,Mathematics Instruction,Metacognition,Outcomes of Education,Problem Solving,Program Effectiveness,Teaching Methods,Theory Practice Relationship,Two Year College Students},
