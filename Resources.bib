  file = {Junior et al_2013_Marxism in Vygotskian approaches to cultural studies of science education.pdf:/Users/christopherpoor/Library/Application Support/Firefox/Profiles/nhlkm2qb.default-1455547641581/zotero/storage/BZHD7ZVN/Junior et al_2013_Marxism in Vygotskian approaches to cultural studies of science education.pdf:application/pdf;Snapshot:/Users/christopherpoor/Library/Application Support/Firefox/Profiles/nhlkm2qb.default-1455547641581/zotero/storage/JWUC5CE5/s11422-013-9485-8.html:text/html}
