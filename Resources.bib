  author = {Goodman, James and Salleh, Ariel},
  urldate = {2016-09-08},
  date = {2013-06},
  pages = {411--424},
  keywords = {ANTI-globalization movement,bio-civilisation,CAPITALISTS & financiers,class,counter-hegemony,ECOFEMINISM,eco-feminism,ecology,Environmental degradation,green economy,GREEN movement,HEGEMONY,meta-industrial labour,neoliberalism,UNCED,WORLD Social Forum},
