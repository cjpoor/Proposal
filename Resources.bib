@article{uslucan_charles_2004,
  title = {Charles {{Sanders Peirce}} and the {{Semiotic Foundation}} of {{Self}} and {{Reason}}},
  volume = {11},
  issn = {1074-9039},
  url = {http://dx.doi.org/10.1207/s15327884mca1102_2},
  doi = {10.1207/s15327884mca1102_2},
  abstract = {The philosophy of the classical American pragmatism represents one of the basic challenges to the conception of self and reason in the history of philosophical and psychological thinking. As the founder of pragmatism, Peirce is well known for his attempt to overcome the Cartesian tradition of philosophy, which was founded on the paradigm of monologic self-consciousness and self-awareness. The dialogical principle is a core piece of the Peircean semiotics, which has deep implications to subjectivity, meaning-construction, reasoning on the "self," and communication. The reinterpretation of the classic tradition of thinking on signs leads Peirce to a triadic and dynamic-dialogical conception of signs. For Peirce, a sign is as such because it stands for something to somebody. It creates in the mind of the person an equivalent or a more developed sign. This radical conception of semiotics terminated in the idea of "man as a sign" and the claim that "men and words reciprocally educate each other." The self is an interpreting subject and an interpreted object. In its innermost being, the self is a communicative agent. The epistemological consequence of this conception is as follows: Truth is closely related to intersubjectivity and the private is synonymous with erroneous.},
  timestamp = {2016-05-10T01:07:30Z},
  number = {2},
  journaltitle = {Mind, Culture, and Activity},
  author = {Uslucan, Haci-Halil},
  urldate = {2016-05-10},
  date = {2004-05-01},
  pages = {96--108},
  file = {Uslucan_2004_Charles Sanders Peirce and the Semiotic Foundation of Self and Reason.pdf:/Users/christopherpoor/Library/Application Support/Firefox/Profiles/nhlkm2qb.default-1455547641581/zotero/storage/X3FFRTNG/Uslucan_2004_Charles Sanders Peirce and the Semiotic Foundation of Self and Reason.pdf:application/pdf;Snapshot:/Users/christopherpoor/Library/Application Support/Firefox/Profiles/nhlkm2qb.default-1455547641581/zotero/storage/9JMNTHRH/s15327884mca1102_2.html:text/html}
}

@article{devries_vygotsky_2000,
  title = {Vygotsky, {{Piaget}}, and {{Education}}: A Reciprocal Assimilation of Theories and Educational Practices},
  volume = {18},
  issn = {0732-118X},
  url = {http://www.sciencedirect.com/science/article/pii/S0732118X00000088},
  doi = {10.1016/S0732-118X(00)00008-8},
  shorttitle = {Vygotsky, {{Piaget}}, and {{Education}}},
  abstract = {Seeking a rapprochement between Vygotskians and Piagetians, the theories of Piaget and Vygotsky are compared, and educational extensions by their followers are examined. A paradox in Vygotsky's theory is highlighted, where evidence is found both for claiming that Vygotsky was a behaviorist and that he was a constructivist. Similarities in the two theories are presented: social factors as having a central role in child development, the transformative nature of internalization, and the individual as what develops. Differences in the theories pertain to the nature of the stimulus, nature and origin of psychological instruments, nature of self-regulation and novelty in development, direction of development, the concept of social development, and the role of language in development. Because practical applications of theories often clarify the theories, some educational extensions of Vygotsky's theory are critiqued from a Piagetian constructivist perspective, and, in contrast, constructivist educational interpretations of Vygotsky's work are noted. Aspects of Piaget's theory emphasized by educators are presented, and educational practices inspired by this theory are outlined. A rapprochement is sought, with consideration of convergences in educational practices of followers of Piaget and Vygotsky, sources of difficulty for rapprochement, and changes necessary in educational theories of followers of both Piaget and Vygotsky.},
  timestamp = {2016-09-21T04:06:31Z},
  issue = {2–3},
  journaltitle = {New Ideas in Psychology},
  shortjournal = {New Ideas in Psychology},
  author = {DeVries, Rheta},
  urldate = {2016-05-10},
  date = {2000-08},
  pages = {187--213},
  file = {DeVries_2000_Vygotsky, Piaget, and Education.pdf:/Users/christopherpoor/Library/Application Support/Firefox/Profiles/nhlkm2qb.default-1455547641581/zotero/storage/ERZFSS7V/DeVries_2000_Vygotsky, Piaget, and Education.pdf:application/pdf;ScienceDirect Snapshot:/Users/christopherpoor/Library/Application Support/Firefox/Profiles/nhlkm2qb.default-1455547641581/zotero/storage/NU6E3AHH/S0732118X00000088.html:text/html}
}

