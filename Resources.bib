  urldate = {2015-05-28},
  date = {2011},
  pages = {11--34},
  keywords = {Education (general),Sociology},
  file = {Baker_2011_The Future of the Schooled Society.pdf:/Users/christopherpoor/Library/Application Support/Firefox/Profiles/nhlkm2qb.default-1455547641581/zotero/storage/M53TZHH2/Baker_2011_The Future of the Schooled Society.pdf:application/pdf;Baker_2011_The Future of the Schooled Society.pdf:/Users/christopherpoor/Library/Application Support/Firefox/Profiles/nhlkm2qb.default-1455547641581/zotero/storage/UUP4C5V4/Baker_2011_The Future of the Schooled Society.pdf:application/pdf;Snapshot:/Users/christopherpoor/Library/Application Support/Firefox/Profiles/nhlkm2qb.default-1455547641581/zotero/storage/SPX246NU/978-94-007-1576-9_2.html:text/html}
