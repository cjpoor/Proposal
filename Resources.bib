@article{mcpherson_child_2005,
  title = {From {{Child}} to {{Musician}}: {{Skill Development During}} the {{Beginning Stages}} of {{Learning}} an {{Instrument}}},
  volume = {33},
  issn = {0305-7356},
  url = {http://search.proquest.com.ezproxy.auckland.ac.nz/docview/1338764/abstract/8245100C52B743D2PQ/1},
  shorttitle = {From {{Child}} to {{Musician}}},
  abstract = {Results of a three-year longitudinal study of 157 children in grades three and four who began studying an instrument in one of eight school music programs are reported. Children were administered tests at the end of each school year to assess their abilities to perform rehearsed music, sight-read, play from memory, play by ear, and improvise. The students' mothers were interviewed in order to calculate how much each subject had practiced his or her instrument. Information was also obtained to help clarify the quality of mental strategies children adopted when performing. The sophistication of children's mental strategies provides an important means for understanding why some progress effortlessly while others struggle and fail. The importance of helping students develop a repertoire of task-appropriate strategies is highlighted.},
  timestamp = {2016-05-23T05:41:04Z},
  langid = {english},
  number = {1},
